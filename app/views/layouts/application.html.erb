<!DOCTYPE html>
<html>
  <head>
    <title>SignIn</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <% if current_user.present?%>
  <%= link_to 'log out', session_destroy_users_path, method: :delete, data: { confirm: 'Are you sure?' } %>
  <% else %>
  <%= link_to 'sign up', new_user_path %> | <%= link_to 'sign in', sign_in_users_path %>
  <% end %>
    <%= yield %>
  </body>
</html>
